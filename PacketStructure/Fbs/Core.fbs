// Basic core structure implelment.

namespace MProject.Core;

// Position, Rotation, Scale, etc...
table Vector {
    x:float;
    y:float;
    z:float;
}

table Quaternion {
    x:float;
    y:float;
    z:float;
    w:float;
}

table Transform {
    position:Vector;
    rotation:Quaternion;
    scale:Vector;
}
