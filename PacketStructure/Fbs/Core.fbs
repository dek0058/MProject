// Basic core structure implelment.

namespace MProject.Packet;

// Position, Rotation, Scale, etc...
table FVector {
    x:float;
    y:float;
    z:float;
}

table FQuaternion {
    x:float;
    y:float;
    z:float;
    w:float;
}

table FTransform {
    position:FVector;
    rotation:FQuaternion;
    scale:FVector;
}
