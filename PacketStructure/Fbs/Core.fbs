// Basic core structure implelment.

namespace MProject.Core;

// Base implement...
struct Header {
    hash_code:[ubyte:20];
    length:uint;
    tag:uint;
}

// Position, Rotation, Scale, etc...
struct Vector3 {
    x:float;
    y:float;
    z:float;
}

struct Transform {
    position:Vector3;
    rotation:Vector3;
    scale:Vector3;
}
